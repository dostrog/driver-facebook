{
    "name": "botman/driver-facebook",
    "license": "MIT",
    "description": "Facebook Messenger driver for BotMan (refreshed by dostrog)",
    "keywords": [
        "Bot",
        "BotMan",
        "Facebook Messenger",
        "Facebook",
        "Messenger"
    ],
    "homepage": "http://github.com/dostrog/driver-facebook",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        },
        {
            "name": "Sergey Donin",
            "email": "sergey.donin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "botman/botman": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dostrog/botman",
            "canonical": false
        }
    ],
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3",
        "illuminate/contracts": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Facebook\\Providers\\FacebookServiceProvider"
            ]
        }
    }
}
